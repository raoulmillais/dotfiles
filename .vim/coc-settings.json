{
  "suggest.floatEnable": true,
  "hover.target": "float",
  "signature.target": "float",
  "diagnostic.enableMessage": "jump",
  "tsserver": {
    "enable": true,
    "log": "verbose"
  },
  "rust-analyzer": {
    "diagnostics": {
      "disabled": [
        "macro-error",
        "unresolved-proc-macro"
      ]
    }
  },
  "coc.source.iced.enable": true,
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"]
    }
  }
}
